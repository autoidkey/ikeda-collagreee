<div class="panel panel-default">

    <div class="panel-header" >
      <% if type == 1 %>
        <h4 class=""><span class="glyphicon glyphicon-tree-deciduous" aria-hidden="true" id="panel-tree"></span> 発散グラフ</h4>
      <% end %>
      <% if type == 2 %>
        <h4 class=""><span class="glyphicon glyphicon-tree-deciduous" aria-hidden="true" id="panel-tree"></span> 収束グラフ</h4>
      <% end %>
      <% if type == 3 %>
        <h4 class=""><span class="glyphicon glyphicon-tree-deciduous" aria-hidden="true" id="panel-tree"></span> 議論ツリー</h4>
      <% end %>

    </div>

      

    <script src="http://d3js.org/d3.v3.min.js"></script>

    <div id="tree-container"　class="container">
      <%= render 'tree_goui', entries: entries, theme: theme , entries: entries , entryAll: entryAll,youyaku: youyaku ,type: type %>
    </div>


</div>