<div class="panel panel-default">
  <div class="panel-body">
    <h3>Users / 閲覧ユーザ</h3>
    <% @theme.users.each do |user| %>
      <%= image_tag user.image_url(:user_thumb), class: 'user-icon img-circle', data: { toggle: "popover", placement: "bottom", content: user.name } %>
    <% end %>
  </div>
</div>
